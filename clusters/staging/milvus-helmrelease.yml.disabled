---
apiVersion: helm.toolkit.fluxcd.io/v2
kind: HelmRelease
metadata:
  name: milvus-database
  namespace: flux-system
spec:
  chart:
    spec:
      chart: milvus
      reconcileStrategy: ChartVersion
      sourceRef:
        kind: HelmRepository
        name: milvus-database
  interval: 10s
  releaseName: milvus
  targetNamespace: milvus
  values:
    pulsar:
      enabled: false
    kafka:
      enabled: true